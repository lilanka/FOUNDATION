#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0-94-ga317a066)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0xf5af60 .scope module, "not_tb" "not_tb" 2 3;
 .timescale 0 0;
v0xfa2f50_0 .var "a", 0 0;
v0xfa2ff0_0 .net "out", 0 0, v0xfa2e30_0;  1 drivers
S_0xf92420 .scope module, "dut" "Not" 2 6, 3 1 0, S_0xf5af60;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out";
    .port_info 1 /INPUT 1 "a";
v0xf5aec0_0 .net "a", 0 0, v0xfa2f50_0;  1 drivers
v0xfa2e30_0 .var "out", 0 0;
E_0xf917b0 .event edge, v0xf5aec0_0;
    .scope S_0xf92420;
T_0 ;
    %wait E_0xf917b0;
    %load/vec4 v0xf5aec0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xfa2e30_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xfa2e30_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xf5af60;
T_1 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xfa2f50_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xfa2f50_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0xf5af60;
T_2 ;
    %vpi_call 2 12 "$monitor", $time, " a=%b, out=%o", v0xfa2f50_0, v0xfa2ff0_0 {0 0 0};
    %vpi_call 2 13 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "not_tb.v";
    "./not.v";
