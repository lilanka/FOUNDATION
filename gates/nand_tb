#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0-94-ga317a066)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x8d1f60 .scope module, "nand_tb" "nand_tb" 2 3;
 .timescale 0 0;
v0x91c1f0_0 .var "a", 0 0;
v0x91c2c0_0 .var "b", 0 0;
v0x91c390_0 .net "out", 0 0, v0x91c0a0_0;  1 drivers
S_0x9098a0 .scope module, "dut" "Nand" 2 6, 3 1 0, S_0x8d1f60;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out";
    .port_info 1 /INPUT 1 "a";
    .port_info 2 /INPUT 1 "b";
v0x909af0_0 .net "a", 0 0, v0x91c1f0_0;  1 drivers
v0x91bfe0_0 .net "b", 0 0, v0x91c2c0_0;  1 drivers
v0x91c0a0_0 .var "out", 0 0;
E_0x908770 .event edge, v0x91bfe0_0, v0x909af0_0;
    .scope S_0x9098a0;
T_0 ;
    %wait E_0x908770;
    %load/vec4 v0x909af0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x91bfe0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c0a0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c0a0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x8d1f60;
T_1 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c1f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c2c0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c1f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c2c0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c1f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c2c0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c1f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c2c0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x8d1f60;
T_2 ;
    %vpi_call 2 14 "$monitor", $time, " a=%b, b=%b, out=%b", v0x91c1f0_0, v0x91c2c0_0, v0x91c390_0 {0 0 0};
    %vpi_call 2 15 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "nand_tb.v";
    "./nand.v";
